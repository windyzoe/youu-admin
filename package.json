{
  "name": "youu-admin",
  "version": "1.0.0",
  "description": "A React OOTB Admin Framework",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env PORT=8181 craco start",
    "build": "cross-env craco build",
    "analyze": "cross-env ANALYZE=true craco build",
    "lint": "eslint --ext .js src mock tests",
    "lint:fix": "eslint --fix --ext .js src mock tests",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "ahooks": "^2.9.2",
    "antd": "^4.17.1",
    "classnames": "^2.2.5",
    "css-animation": "^2.0.0",
    "dva": "^2.6.0-beta.22",
    "dva-loading": "^3.0.22",
    "echarts": "^5.1.1",
    "echarts-for-react": "^3.0.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.19.3",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.5.10",
    "qiankun": "^2.3.5",
    "qs": "^6.5.0",
    "react": "^17.0.1",
    "react-document-title": "2.0.3",
    "react-dom": "^17.0.1",
    "react-intl-universal": "^2.4.6",
    "react-particles-js": "^3.4.1",
    "tsparticles": "^1.30.0",
    "umi-request": "^1.3.0",
    "url-polyfill": "^1.0.10",
    "wangeditor": "^4.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^3.1.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "husky": "^4.3.0",
    "lint-staged": "^7.2.0",
    "mocker-api": "^2.7.4",
    "prettier": "^2.2.1",
    "less": "^3.8.1",
    "less-loader": "^7.0.0",
    "@craco/craco": "^6.0.0",
    "react-scripts": "^4.0.1",
    "webpackbar": "^5.0.0-3",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    "chrome > 75"
  ]
}
